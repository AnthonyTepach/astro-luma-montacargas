---
const logo = "../../public/logo-luma-montacargas-temp.png"
---

<header aria-label="Page Header " >
    <div class="mx-auto max-w-screen-xl px-4 py-8 sm:py-12 sm:px-6 lg:px-8 ">
      <div class="sm:flex sm:items-center sm:justify-between">
        <div class="text-center sm:text-left">
          <img class="w-44 md:w-60 lg:w-72" src={logo} alt="logo-montacargas">
  
         
        </div>
  
        <div class="mt-4 flex flex-col gap-4 sm:mt-0 sm:flex-row sm:items-center">
            <div class="text-center sm:text-left">
                <p class="mt-1.5 text-sm text-green-500"><strong>Pradera de los Jazmines, 45679 Guadalajara, Jal.</strong></p>
              </div>
        </div>
      </div>
    </div>
  </header>
  